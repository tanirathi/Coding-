class Solution
{
	public:
	string is_palindrome(int n) {
    int original = n;  // Store the original number
    int reversed = 0;
    
    // Reverse the number
    while (n > 0) {
        int digit = n % 10;
        reversed = reversed * 10 + digit;
        n /= 10;
    }
    
    // Check if the original number is the same as the reversed number
    if(original == reversed){
        return "Yes";   
    }
    else{
        return "No";
    }
 }
};
